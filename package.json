{
    "name": "js-ilogger",
    "version": "1.0.0",
    "description": "Cross-platform logger for javascript, built in the spirit of Microsoft's ILogger interface.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "mocha -r ts-node/register tests/**/*.test.ts",
        "lint": "yarn eslint **/*.ts --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
    },
    "keywords": [
        "log",
        "console",
        "logger",
        "logging"
    ],
    "author": "Jesse Mursky-Fuller",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/mocha": "^8.0.4",
        "@types/node": "^14.14.10",
        "@types/sinon": "^9.0.9",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "chai": "^4.2.0",
        "eslint": "^7.14.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "mocha": "^8.2.1",
        "prettier": "^2.2.1",
        "sinon": "^9.2.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.2"
    }
}
